<header>
  <% unless dashboard.nil? %>
  <div class="linkBack">
    <a href="<%= dashboard %>" class="link">
      <i class="far fa-chevron-left"></i>
      Dashboard
    </a>
  </div>
  <% end %>
  <div class="title-container">
    <div class="box">
      <h1 class="title"><%= title %></h1>
      <% unless create.nil? %>
        <a href='<%= create[:path] %>' class='button'>
          <i class="far fa-plus-octagon"></i> Create A New <%= create[:object] %>
        </a>
      <% end %>
    </div>
  </div>
</header>
